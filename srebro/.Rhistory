#============================================================
#TRETJA NALOGA - Eksponentno glajenje
#============================================================
#------------------------------------------------------------
#(a) Funkcija EG(vrsta, alpha)
#------------------------------------------------------------
EG <- function(vrsta, alpha) {
glajena_vrsta <- c(vrsta[1])
l <- length(vrsta)
for (i in 2:l) {
glajena_vrsta[i] <- alpha*vrsta[i] + (1-alpha)*glajena_vrsta[i-1]
}
return(ts(glajena_vrsta))
}
